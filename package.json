  "scripts": {
    "test": "cross-env TZ=utc NODE_ENV=test jest --testPathIgnorePatterns=src/__tests__/support",
    "test:debug": "cross-env TZ=utc NODE_ENV=test jest --inspect --runInBand --no-cache --testPathIgnorePatterns=src/__tests__/support",
    "prepublishOnly": "yarn run build",
    "build": "rollup -c",
    "lint": "eslint ./src --max-warnings 0",
    "beauty_check": "prettier --check ./src",
    "prettify": "prettier --write 'src/**/*.{js,jsx,json,css,scss,md}'",
    "storybook": "storybook dev -p 6006"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
